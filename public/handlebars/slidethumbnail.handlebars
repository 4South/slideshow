{{#if App.godMode}}
  <button class="btn btn-danger"{{action 'delete' view.content}}>
    X
  </button>
  <button class="btn btn-primary"{{action 'moveUp' view.content}}>
    UP 
  </button>
  <button class="btn btn-primary"{{action 'moveDown' view.content}}>
    DN 
  </button>
{{/if}}
<span class="lead">{{view.content.name}}</span>
<span class="lead">{{view.content.position}}</span>
